@Article{mooers2009crystallographicstudiesofdnaandrna,
  author         = {Mooers, B. H.},
  title          = {Crystallographic studies of DNA and RNA},
  journal        = {Methods},
  year           = {2009},
  volume         = {47},
  number         = {3},
  pages          = {168-176},
  month          = {Mar},
  note           = {[DOI:\href{http://dx.doi.org/10.1016/j.ymeth.2008.09.006}{10.1016/j.ymeth.2008.09.006}] [PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/18848992}{18848992}]},
  abstract       = {Our knowledge of nucleic acid structure grew rapidly over the past decade with the determination to high resolution of larger structures of great biological significance. Advances in sample preparation, crystallization techniques, cryocrystallography, access to synchrotron radiation, and crystallographic software continue to accelerate the structure determination of nucleic acids. Crystallographic studies of DNA and RNA molecules share many considerations that we outline here. The application of crystallography to RNA is illustrated with the structure determination of the CUG repeat that is linked to type I myotonic dystrophy.},
