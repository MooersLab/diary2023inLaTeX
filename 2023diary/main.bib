@article{yan2014primingoftoll-likereceptor4pathwayinmesenchymalstemcellsincreasesexpressionofbcellactivatingfactora,

   Author = {Yan, H. and Wu, M. and Yuan, Y. and Wang, Z. Z. and Jiang, H. and Chen, T.},

