@Article{korb2007antcolonyoptimiationapproachtoproteinliganddocking,
  author   = {Korb, Oliver and St{\"u}tzle, Thomas and Exner, Thomas E.},
  title    = {An ant colony optimization approach to flexible protein--ligand docking},
