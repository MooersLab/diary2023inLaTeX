@article{mokdad2006structuralandevolutionaryclassificationofg/uwobblebasepairsintheribosome,

   Author = {Mokdad, A. and Krasovska, M. V. and Sponer, J. and Leontis, N. B.},

   Title = {Structural and evolutionary classification of G/U wobble basepairs in the ribosome},

   Journal = {Nucleic Acids Res},

   Volume = {34},

