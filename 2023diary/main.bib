@article{huang2016potentantigen-adjuvantdeliverysystembyconjugationofmycobacteriumtuberculosisag85b-hspxfusionproteinwitharabinogalactan-polyi:cconjugatea,

   Author = {Huang, Q. and Yu, W. and Hu, T.},

   Title = {Potent Antigen-Adjuvant Delivery System by Conjugation of Mycobacterium tuberculosis Ag85B-HspX Fusion Protein with Arabinogalactan-Poly(I:C) Conjugate},

   Journal = {Bioconjug. Chem.},

   Volume = {27},

   Number = {4},

   Pages = {1165-1174},

