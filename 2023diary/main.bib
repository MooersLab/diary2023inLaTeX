@Article{cornilescu2016,
  author   = {Gabriel Cornilescu and Allison L. Didychuk and Margaret L. Rodgers and Lauren A. Michael and Jordan E. Burke and Eric J. Montemayor and Aaron A. Hoskins and Samuel E. Butcher},
  title    = {Structural Analysis of Multi-Helical \{RNAs\} by NMR SAXS/WAXS: Application to the U4/U6 di-snRNA},
  journal  = {Journal of Molecular Biology},
  year     = {2016},
  volume   = {428},
  number   = {5, Part A},
  pages    = {777 - 789},
  note     = {Challenges in \{RNA\} Structural Modeling and Design},
  abstract = {Abstract \{NMR\} and \{SAXS\} (small-angle X-ray scattering)/WAXS (wide-angle X-ray scattering) are highly complementary approaches for the analysis of \{RNA\} structure in solution. Here we describe an efficient NMR SAXS/WAXS approach for structural investigation of multi-helical RNAs. We illustrate this approach by determining the overall fold of a 92-nt 3-helix junction from the U4/U6 di-snRNA. The U4/U6 di-snRNA is conserved in eukaryotes and is part of the U4/U6.U5 tri-snRNP, a large ribonucleoprotein complex that comprises a major subunit of the assembled spliceosome. Helical orientations can be determined by X-ray scattering data alone, but the addition of \{NMR\} \{RDC\} (residual dipolar coupling) restraints improves the structure models. \{RDCs\} were measured in two different external alignment media and also by magnetic susceptibility anisotropy. The resulting alignment tensors are collinear, which is a previously noted problem for nucleic acids. Including \{WAXS\} data in the calculations produces models with significantly better fits to the scattering data. In solution, the U4/U6 di-snRNA forms a 3-helix junction with a planar Y-shaped structure and has no detectable tertiary interactions. Single-molecule Forrster resonance energy transfer data support the observed topology. A comparison with the recently determined cryo-electron microscopy structure of the U4/U6.U5 tri-snRNP illustrates how proteins scaffold the \{RNA\} and dramatically alter the geometry of the U4/U6 3-helix junction. },
  doi      = {http://dx.doi.org/10.1016/j.jmb.2015.11.026},
  issn     = {0022-2836},
  keywords = {RNA, NMR, SAXS, structure modeling, spliceosome },
  url      = {http://www.sciencedirect.com/science/article/pii/S0022283615006889},
}

