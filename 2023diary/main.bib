@article{matsuzaki2015interleukin-17aandtoll-likereceptor3ligandpolyi:csynergisticallyinducedneutrophilchemoattractantproductionbybronchialepithelialcellsa,

   Author = {Matsuzaki, H. and Mikami, Y. and Makita, K. and Takeshima, H. and Horie, M. and Noguchi, S. and Jo, T. and Narumoto, O. and Kohyama, T. and Takizawa, H. and Nagase, T. and Yamauchi, Y.},

