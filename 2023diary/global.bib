  shorttitle     = {Molecular replacement with MOLREP},
}

@Article{Vagin2001SphericallyAveragedPhasedTranslationFunctionAndItsApplicationToTheSearchForMoleculesAndFragmentsInElectronDensityMaps,
  author         = {Vagin, A. A. and Isupov, M. N.},
