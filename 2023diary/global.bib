@Article{Johnson20143DMolecularModelsOfWholeHIV1VirionsGeneratedWithCellPACK,
  author          = {Johnson, Graham T and Goodsell, David S and Autin, Ludovic and Forli, Stefano and Sanner, Michel F and Olson, Arthur J},
  journal         = {Faraday discussions},
