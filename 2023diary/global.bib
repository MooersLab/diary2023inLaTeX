@Article{Landweber1994PhylogeneticAnalysisOfRNAEditingAPrimitiveGeneticPhenomenon,
  author         = {Landweber, L. F. and Gilbert, W.},
  title          = {Phylogenetic analysis of RNA editing: a primitive genetic phenomenon},
  journal        = {Proc. Natl. Acad. Sci. U.S.A.},
  year           = {1994},
  volume         = {91},
  number         = {3},
  pages          = {918-921},
  month          = {Feb},
  note           = {[PubMed Central:\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC521424}{PMC521424}] [PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/8302867}{8302867}]},
  abstract       = {RNA editing by extensive uridine addition and deletion creates over 90% of the amino acid codons in the cytochrome-c oxidase subunit III (COIII) transcript in Trypanosoma brucei and Herpetomonas, whereas editing of the COIII transcripts in Leishmania tarentolae and Crithidia fasciculata generates only 6% of the amino acid codons and is limited to the 5' ends. Is extensive RNA editing a primitive or derived character? We constructed a phylogenetic tree based on nuclear small-subunit and mitochondrial large- and small-subunit ribosomal RNA sequences for nine species of kinetoplastid protozoa. Our results suggest that extensive editing is a primitive genetic phenomenon that has disappeared in recent evolutionary time and also that there have been multiple losses of the digenetic lifestyle by loss of the vertebrate host in parasite evolution.},
  endnotereftype = {Journal Article},
  owner          = {blaine-mooers},
  shorttitle     = {Phylogenetic analysis of RNA editing: a primitive genetic phenomenon},
}

