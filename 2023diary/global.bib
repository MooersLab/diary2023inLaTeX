@Article{Mooers2016DirectMethodsStructureDeterminationOfATrypanosomeRNAEditingSubstrateFragmentWithTranslationalPseudosymmetry,
  author         = {Mooers, B. H.},
  title          = {Direct-methods structure determination of a trypanosome RNA-editing substrate fragment with translational pseudosymmetry},
  journal        = {Acta Crystallogr D Struct Biol},
  year           = {2016},
