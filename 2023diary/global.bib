@Article{Nguyen2017NGLviewInteractiveMolecularGraphicsForJupyterNotebooks,
  author    = {Nguyen, Hai and Case, David A and Rose, Alexander S},
  journal   = {Bioinformatics},
  title     = {NGLview--interactive molecular graphics for Jupyter notebooks},
  year      = {2017},
  number    = {7},
  pages     = {1241--1242},
  volume    = {34},
  abstract  = {NGLview is a Jupyter/IPython widget to interactively view molecular structures as well as trajectories from molecular dynamics simulations. Fast and scalable molecular graphics are provided through the NGL Viewer. The widget supports showing data from the file-system, online data bases and from objects of many popular analysis libraries including mdanalysis, mdtraj, pytraj, rdkit and more. The source code is freely available under the MIT license at https://github.com/arose/nglview. Python packages are available from PyPI and bioconda. NGLview uses Python on the server-side and JavaScript on the client. The integration with Jupyter is done through the ipywidgets package. The NGL Viewer is embedded client-side to provide WebGL accelerated molecular graphics.},
  annote    = {NGLview is a Python package that uses Python on the server and Javascript on the client. The ipywidgets package integrates Nglview with Jupyter. The NGLViewer uses WebGL accelerated molecular graphics so it embedded on the client-side. Interactive computing allows an exploratory process where each computation yields results that guide the next step. Jupyter Notebooks have been adopted by some research communities to analyze and share data. Code and data are put into context by in-line output and flanking explanatory prose. In contrast, standalone molecular graphics packages integrate visualization and computations via plugins. Jupyter notebooks enable a modular approach with greater intermingling of multiple software packages and libraries. Other tools that integrate molecular graphics in Jupyter notebooks include 3dmol.js (Rego and Koes, 2015), chemview (https://github.com/gabrielelanaro/chemview) and pv (https://github.com/biasmv/pv). Large trajectories are sent to Jupyter frame by frame. Although NGLView can read in a number of structure and trajectory data types, custom adaptors can be built for any structure or trajectory data type. The user just needs to write a class that implements the Structure or Trajectory interface from the base\_adaptor. Objects of such classes can be passed to the NGLWidget contructor to create a view for that data, so you have to know the basics of object oriented programming to make such customizations. The documentation is found here (http://nglviewer.org/nglview/latest/api.html).},
  doi       = {10.1093/bioinformatics/btx789},
  publisher = {Oxford University Press},
  url       = {file:///Users/blaine/0000pdfLibraryTmp/Nguyen2018NGLviewInteractiveMolecularGraphicsForJupyterNotebooks.pdf},
}

