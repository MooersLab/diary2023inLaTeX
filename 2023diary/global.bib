@Article{Chalker2020OpenOnDemandStateOfThePlatformProjectAndTheFuture,
  author   = {Chalker, Alan and Franz, Eric and Rodgers, Morgan and Dockendorf, Trey and Johnson, Doug and Sajdak, Doris and White, Joseph P. and Plessinger, Benjamin D. and Zia, Mohammad and Gallo, Steven M. and Settlage, Robert E. and Hudak, David E.},
