@Article{Komeda2011ThePhosphateClampASmallAndIndependentMotifForNucleicAcidBackboneRecognition,
  author         = {Komeda, Seiji and Moulaei, Tinoush and Chikuma, Masahiko and Odani, Akira and Kipping, Ralph and Farrell, Nicholas P. and Williams, Loren Dean},
  title          = {The phosphate clamp: a small and independent motif for nucleic acid backbone recognition},
  journal        = {Nucleic Acids Research},
  year           = {2011},
  volume         = {39},
  number         = {1},
  pages          = {325-336},
  abstract       = {The 1.7âÃÃ¢âÃ X-ray crystal structure of the B-DNA dodecamer, [d(CGCGAATTCGCG)]2 (DDD)-bound non-covalently to a platinum(II) complex, [{Pt(NH3)3}2-Â¬Âµ-{trans-Pt(NH3)2(NH2(CH2)6NH2)2}](NO3)6 (1, TriplatinNC-A,) shows the trinuclear cation extended along the phosphate backbone and bridging the minor groove. The square planar tetra-am(m)ine Pt(II) units form bidentate N-O-N complexes with OP atoms, in a Phosphate Clamp motif. The geometry is conserved and the interaction prefers O2P over O1P atoms (frequency of interaction is O2PâÃÃ¢>âÃÃ¢O1P, base and sugar oxygensâÃÃ¢>âÃÃ¢N). The binding mode is very similar to that reported for the DDD and [{trans-Pt(NH3)2(NH2(CH2)6(NH3+)}2-Â¬Âµ-{trans-Pt(NH3)2(NH2(CH2)6NH2)2}](NO3)8 (3, TriplatinNC), which exhibits in vivo anti-tumour activity. In the present case, only three sets of Phosphate Clamps were found because one of the three Pt(II) coordination spheres was not clearly observed and was characterized as a bare Pt2+ ion. Based on the electron density, the relative occupancy of DDD and the sum of three Pt(II) atoms in the DDD-1 complex was 1:1.69, whereas the ratio for DDD-2 was 1:2.85, almost the mixing ratio in the crystallization drop. The high repetition and geometric regularity of the motif suggests that it can be developed as a modular nucleic acid binding device with general utility.},
  endnotereftype = {Journal Article},
  owner          = {blaine-mooers},
  shorttitle     = {The phosphate clamp: a small and independent motif for nucleic acid backbone recognition},
  url            = {http://nar.oxfordjournals.org/content/39/1/325.abstract},
}

