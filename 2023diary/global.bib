@Article{Panjkovich2016SASpyAPyMOLPluginForManipulationAndRefinementOfHybridModelsAgainstSmallAngleXRayScatteringData,
  author    = {Panjkovich, Alejandro and Svergun, Dmitri I.},
  title     = {SASpy: a PyMOL plugin for manipulation and refinement of hybrid models against small angle X-ray scattering data},
  journal   = {Bioinformatics},
  year      = {2016},
  volume    = {32},
  number    = {13},
  pages     = {2062--2064},
  abstract  = {Summary: Complex formation and conformational transitions of biological macromolecules in solution can be effectively studied using the information about overall shape and size provided by small angle X-ray scattering (SAXS). Hybrid modeling is often applied to integrate high-resolution models into SAXS data analysis. To facilitate this task, we present SASpy, a PyMOL plugin that provides an easy-to-use graphical interface for SAXS-based hybrid modeling. Through a few mouse clicks in SASpy, low-resolution models can be superimposed to high-resolution structures, theoretical scattering profiles and fits can be calculated and displayed on-the-fly. Mouse-based manual rearrangements of complexes are conveniently applied to rapidly check and interactively refine tentative models. Interfaces to automated rigid-body and flexible refinement of macromolecular models against the experimental SAXS data are provided.Availability: SASpy is available as open source at: github.com/emblsaxs/saspy/. Working installations of
both PyMOL (www.pymol.org) and ATSAS (www.embl-hamburg.de/biosaxs/download.html) are required.Contact: apanjkovich@embl-hamburg.de, svergun@embl-hamburg.de},
  doi       = {10.1093/bioinformatics/btw071},
  eprint    = {http://bioinformatics.oxfordjournals.org/content/early/2016/02/28/bioinformatics.btw071.full.pdf+html},
  publisher = {Oxford University Press},
  url       = {http://bioinformatics.oxfordjournals.org/content/early/2016/02/28/bioinformatics.btw071.abstract},
}

@Article{Harp2016PhosphorusSADPhasingForNucleicAcidStructuresLimitationsAndPotential,
  author    = {Harp, Joel M. and Pallan, Pradeep S. and Egli, Martin},
  title     = {Phosphorus SAD Phasing for Nucleic Acid Structures: Limitations and Potential},
  journal   = {Crystals},
  year      = {2016},
  volume    = {6},
  number    = {10},
  pages     = {125},
  abstract  = {Phasing of nucleic acid crystal diffraction data using the anomalous signal of phosphorus, P-SAD, at Cukα wavelength has been previously demonstrated using Z-DNA. Since the original work on P-SAD with Z-DNA there has been, with a notable exception, a conspicuous absence of applications of the technique to additional nucleic acid crystal structures. We have reproduced the P-SAD phasing of Z-DNA using a rotating-anode source and have attempted to phase a variety of nucleic acid crystals using P-SAD without success. A comparison of P-SAD using Z-DNA and a representative nucleic acid, the Dickerson-Drew dodecamer, is presented along with a S-SAD using only two sulfurs to phase a 2’-thio modified DNA decamer. A theoretical explanation for the limitation of P-SAD applied to nucleic acids is presented to show that the relatively high atomic displacement parameter of phosphorus in the nucleic acid backbone is responsible for the lack of success in applying P-SAD to nucleic acid diffraction data.},
  annote    = {I spent an hour reading the paper by Joel Harp and Martin Egli of the failure of of P-phasing of B-form DNA. I had selected as an example paper from Crystals. This is a surprisingly good paper. Of course, this paper meets Martin Egli's standard of excellence. This paper does describe their success at P-SAD phasing an RNA with two sulfurs incorporated. They also have success at Direct Methods with SIR2004 but they did not mention success with shelxd. Their analysis was extensive of relative phosphorous mobility is heroic. They even attempted 10,000 structure determination with ShelxE! This paper is very relevant to my PSAD and ISAD phasing paper. Their introduction has an excellent description of why nucleic acids are difficult to derivative. They also compare delta B of sulfur and phosphorous atoms versus the light atoms for proteins and nucleic acids. They found that sulfurs in proteins on average have lower B-values while phosphorous atoms in nucleic acids have higher B-values. They attribute the failure to the greater mobility of the phosphates compared to the other atoms as assessed from the B-values. I suspect that A-form helices will show less mobility in the phosphates. They also have a nice review of progress with XFELS.},
  doi       = {10.3390/cryst6100125},
  publisher = {Multidisciplinary Digital Publishing Institute},
  url       = {http://dx.doi.org/10.3390/cryst6100125},
}

