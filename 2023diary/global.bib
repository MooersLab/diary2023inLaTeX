}

@Article{Pirikahu2016BayesianMethodsOfConfidenceIntervalConstructionForThePopulationAttributableRiskFromCrossSectionalStudies,
  author          = {Pirikahu, Sarah and Jones, Geoffrey and Hazelton, Martin L and Heuer, Cord},
  title           = {Bayesian methods of confidence interval construction for the population attributable risk from cross-sectional studies.},
