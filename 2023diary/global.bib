@Article{Yang2016TheRNABindingProteinMex3BIsACoreceptorOfTollLikeReceptor3InInnateAntiviralResponse,
  author         = {Yang, Y. and Wang, S. Y. and Huang, Z. F. and Zou, H. M. and Yan, B. R. and Luo, W. W. and Wang, Y. Y.},
  title          = {The RNA-binding protein Mex3B is a coreceptor of Toll-like receptor 3 in innate antiviral response},
  journal        = {Cell Res.},
  year           = {2016},
  volume         = {26},
  number         = {3},
  pages          = {288-303},
  month          = {Mar},
  note           = {[PubMed Central:\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4783467}{PMC4783467}] [DOI:\href{http://dx.doi.org/10.1038/cr.2016.16}{10.1038/cr.2016.16}] [PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/26823206}{26823206}]},
  abstract       = {Recognition of viral dsRNA by Toll-like receptor 3 (TLR3) leads to induction of interferons (IFNs) and proinflammatory cytokines, and innate antiviral response. Here we identified the RNA-binding protein Mex3B as a positive regulator of TLR3-mediated signaling by expression cloning screens. Cells from Mex3b(-/-) mice exhibited reduced production of IFN-âÃ©Â¬â¤ in response to the dsRNA analog poly(I:C) but not infection with RNA viruses. Mex3b(-/-) mice injected with poly(I:C) was more resistant to poly(I:C)-induced death. Mex3B was associated with TLR3 in the endosomes. It bound to dsRNA and increased the dsRNA-binding activity of TLR3. Mex3B also promoted the proteolytic processing of TLR3, which is critical for its activation. Mutants of Mex3B lacking its RNA-binding activity inhibited TLR3-mediated IFN-âÃ©Â¬â¤ induction. These findings suggest that Mex3B acts as a coreceptor of TLR3 in innate antiviral response.},
  endnotereftype = {Journal Article},
  owner          = {blaine-mooers},
  shorttitle     = {The RNA-binding protein Mex3B is a coreceptor of Toll-like receptor 3 in innate antiviral response},
}

