@Article{Erixon2003ReliabilityOfBayesianPosteriorProbabilitiesAndBootstrapFrequenciesInPhylogenetics,
  author          = {Erixon, Per and Svennblad, Bodil and Britton, Tom and Oxelman, Bengt},
  title           = {Reliability of Bayesian posterior probabilities and bootstrap frequencies in phylogenetics.},
