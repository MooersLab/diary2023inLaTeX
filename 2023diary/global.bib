@Article{Keel2007AGeneralStrategyToSolveThePhaseProblemInRNACrystallography,
  author    = {Keel, Amanda Y and Rambo, Robert P and Batey, Robert T and Kieft, Jeffrey S},
  journal   = {Structure},
  title     = {A general strategy to solve the phase problem in RNA crystallography},
  year      = {2007},
  number    = {7},
  pages     = {761--772},
  volume    = {15},
  abstract  = {X-ray crystallography of biologically important RNA molecules has been hampered by technical challenges, including finding heavy-atom derivatives to obtain high-quality experimental phase information. Existing techniques have drawbacks, limiting the rate at which important new structures are solved. To address this, we have developed a reliable means to localize heavy atoms specifically to virtually any RNA. By solving the crystal structures of thirteen variants of the G*U wobble pair cation binding motif, we have identified a version that when inserted into an RNA helix introduces a high-occupancy cation binding site suitable for phasing. This "directed soaking" strategy can be integrated fully into existing RNA crystallography methods, potentially increasing the rate at which important structures are solved and facilitating routine solving of structures using Cu-Kalpha radiation. This method already has been used to solve several crystal structures.},
  annote    = {The strategy here is to introduce certain sequence motif involving GU wobble pairs that bind to cobalt hexammine. The phase problem is much easier to solve if you know in advance the number of bound heavy atoms and the position in the RNA sequence of the heavier atom binding site.},
  doi       = {10.1016/j.str.2007.06.003},
  publisher = {Elsevier},
  url       = {https://www.sciencedirect.com/science/article/pii/S0969212607002080?via%3Dihub},
}

