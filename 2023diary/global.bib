  keywords        = {Catalytic Domain; Computer Simulation; Computer-Aided Design; Drug Design; HIV, enzymology; HIV Infections, drug therapy, enzymology, virology; HIV Integrase, chemistry, metabolism; HIV Integrase Inhibitors, pharmacology; Humans; Models, Molecular; Models, Statistical; Protein Binding},
  mid             = {NIHMS662727},
