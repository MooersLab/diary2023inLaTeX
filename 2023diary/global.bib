@Article{Hu2015ActivationOfTollLikeReceptor3InducesSpermatogonialStemCellApoptosis,
  author         = {Hu, J. and Song, D. and Luo, G. and Xu, S. and Cao, Y. and Sun, Z.},
  title          = {Activation of Toll like receptor 3 induces spermatogonial stem cell apoptosis},
  journal        = {Cell Biochem. Funct.},
  year           = {2015},
  volume         = {33},
  number         = {6},
  pages          = {415-420},
  month          = {Aug},
  note           = {[DOI:\href{http://dx.doi.org/10.1002/cbf.3133}{10.1002/cbf.3133}] [PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/26350376}{26350376}]},
  abstract       = {Germ cell apoptosis may be associated with the male infertility. The pathogenesis is to be further understood. Viral infection is one of the causative factors of apoptosis of the body cells. This study aims to investigate the role of activation of Toll like receptor (TLR)3 in the induction of germ cell apoptosis. In this study, spermatogonial stem cells (SSC) were isolated from C57BL/6 mouse testes. The expression of TLR3 on SSC was by RT-qPCR and Western blotting. polyinosinic-polycytidylic acid (Poly I:C) was employed to activate TLR3 on SSCs. The results showed that re-activation by PolyI:C induced SSC apoptosis. Exposure to PolyI:C induced interferon regulatory factor 3 (IRF3) phosphorylation in SSCs. TLR3 and IRF3 formed a complex in the SSCs. The complex of TLR3/IRF3 bound to the promoter of Fas ligand and promoted Fas ligand expression in SSC, and thus induced SSC apoptosis. In conclusion, the results of the present study indicate that activation of TLR3 by PolyI:C induces the SSC apoptosis, which implies that viral infection may interfere with the male germ cell development.},
  endnotereftype = {Journal Article},
  owner          = {blaine-mooers},
  shorttitle     = {Activation of Toll like receptor 3 induces spermatogonial stem cell apoptosis},
}

