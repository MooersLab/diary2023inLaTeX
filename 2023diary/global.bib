@Article{Liu2012ASyntheticDoubleStrandedRNAPolyICInducesARapidApoptosisOfHumanCD34Cells,
  author         = {Liu, J. and Guo, Y. M. and Hirokawa, M. and Iwamoto, K. and Ubukawa, K. and Michishita, Y. and Fujishima, N. and Tagawa, H. and Takahashi, N. and Xiao, W. and Yamashita, J. and Ohteki, T. and Sawada, K.},
  title          = {A synthetic double-stranded RNA, poly I:C, induces a rapid apoptosis of human CD34(+) cells},
  journal        = {Exp. Hematol.},
  year           = {2012},
  volume         = {40},
  number         = {4},
  pages          = {330-341},
  month          = {Apr},
  note           = {[DOI:\href{http://dx.doi.org/10.1016/j.exphem.2011.12.002}{10.1016/j.exphem.2011.12.002}] [PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/22198151}{22198151}]},
  abstract       = {Toll-like receptor 3 (TLR3), retinoic acid-inducible gene I, and melanoma differentiation-associated antigen 5 (RIG-I/MDA-5) helicases are known to sense double-stranded RNA (dsRNA) virus and initiate antiviral responses, such as production of type-I interferons (IFNs). Recognition of dsRNA by TLR3 or RIG-I/MDA-5 is cell-type-dependent and recent studies have shown a direct link between TLRs and hematopoiesis. We hypothesized that viral dsRNA recognized by either TLR3 or RIG-I/MDA-5, affects the growth of human hematopoietic stem/progenitor cells. Here we show that polyinosinic polycytidylic acid (poly I:C)-mediated very rapid apoptosis occurs within 1 hour in CD34(+) cells in a dose-dependent manner. Polyadenylic-polyuridylic acid, another synthetic dsRNA that signals only through TLR3, had no effect. Poly I:C-LMW/LyoVec, a complex between low molecular-weight poly I:C and the transfection reagent LyoVec, which signals only through RIG-I/MDA-5, induces apoptosis of CD34(+) cells. A strong and sustained upregulation of messenger RNA and protein levels of Noxa, a proapoptotic BH3-only protein that can be induced by RIG-I/MDA-5 pathway, is found in CD34(+) cells treated by poly I:C. Although poly I:C upregulates type-I IFNs in CD34(+) cells, neither exogenous IFN-âÃ©Â¬Â± nor IFN-âÃ©Â¬â¤ induces rapid apoptosis in CD34(+) cells and neutralization or blocking of type-I IFN receptor does not rescue CD34(+) cells, whereas Z-VAD, a pan-caspase inhibitor, rescues the cells from apoptosis. These results suggest that RIG-I/MDA-5, but not TLR3, signaling triggers poly I:C-induced rapid apoptosis of human CD34(+) cells, which will provide an insight into the mechanisms of dsRNA virus-mediated hematopoietic disorders.},
  endnotereftype = {Journal Article},
  owner          = {blaine-mooers},
  shorttitle     = {A synthetic double-stranded RNA, poly I:C, induces a rapid apoptosis of human CD34(+) cells},
}

