@InProceedings{Koenzen2020CodeDuplicationAndReuseInJupyterNotebooks,
  author       = {Koenzen, Andreas P and Ernst, Neil A and Storey, Margaret-Anne D},
  booktitle    = {2020 IEEE Symposium on Visual Languages and Human-Centric Computing (VL/HCC)},
  title        = {Code duplication and reuse in Jupyter notebooks},
  year         = {2020},
  organization = {IEEE},
  pages        = {1--9},
  annote       = {The study surveyed GitHub repositories that contained one or with Juptyer notebooks. They randomly sampled 1000 such GitHub repositories. Three questions were addressed: how much cell code duplication occurs in notebooks, how does code reuse occur, and what are the preferred sources of code reuse in Jupyter notebooks. They found that one in 13 code cells were duplicates. The four top categories of duplicated code snippets were visualization (21%), machine learning (15%), the definition of functions (12%), and data science (9%). The main method of obtaining code to reuse was by web browsing. The top three kinds of sites visited were tutorial sites (35%), API documentation (32%), and stack overflow (14%). Interestingly, the use of snippet libraries was not mentioned. This suggests that most users of Juptyer notebooks are not taking advantage of the existing nb extensions that support code snippets.},
}

