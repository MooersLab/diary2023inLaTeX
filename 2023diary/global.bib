@Article{Luft2007EfficientOptimizationOfCrystallizationConditionsByManipulationOfDropVolumeRatioAndTemperature,
  author   = {Luft, Joseph R. and Wolfley, Jennifer R. and Said, Meriem I. and Nagel, Raymond M. and Lauricella, Angela M. and Smith, Jennifer L. and Thayer, Max H. and Veatch, Christina K. and Snell, Edward H. and Malkowski, Michael G. and DeTitta, George T.},
  journal  = {Protein Science},
  title    = {Efficient optimization of crystallization conditions by manipulation of drop volume ratio and temperature},
  year     = {2007},
  number   = {4},
  pages    = {715--722},
  volume   = {16},
  abstract = {An efficient optimization method for the crystallization of biological macromolecules has been developed and tested. This builds on a successful high-throughput technique for the determination of initial crystallization conditions. The optimization method takes an initial condition identified through screening and then varies the concentration of the macromolecule, precipitant, and the growth temperature in a systematic manner. The amount of sample and number of steps is minimized and no biochemical reformulation is required. In the current application a robotic liquid handling system enables high-throughput use, but the technique can easily be adapted in a nonautomated setting. This method has been applied successfully for the rapid optimization of crystallization conditions in nine representative cases.},
  annote   = {They used a crystallization robot to vary the reservior to protein solution ratio in both directions. They stopped a 1:4 and 4:1.},
  doi      = {10.1110/ps.062699707},
  url      = {http:https://dx.doi.org/10.1110/ps.062699707},
}

