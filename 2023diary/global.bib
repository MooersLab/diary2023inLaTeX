@Article{Matsuzaki2015Interleukin17AAndTollLikeReceptor3LigandPolyICSynergisticallyInducedNeutrophilChemoattractantProductionByBronchialEpithelialCells,
  author         = {Matsuzaki, H. and Mikami, Y. and Makita, K. and Takeshima, H. and Horie, M. and Noguchi, S. and Jo, T. and Narumoto, O. and Kohyama, T. and Takizawa, H. and Nagase, T. and Yamauchi, Y.},
  title          = {Interleukin-17A and Toll-Like Receptor 3 Ligand Poly(I:C) Synergistically Induced Neutrophil Chemoattractant Production by Bronchial Epithelial Cells},
  journal        = {PLoS ONE},
  year           = {2015},
  volume         = {10},
  number         = {10},
  pages          = {e0141746},
  note           = {[PubMed Central:\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4624416}{PMC4624416}] [DOI:\href{http://dx.doi.org/10.1371/journal.pone.0141746}{10.1371/journal.pone.0141746}] [PubMed:\href{http://www.ncbi.nlm.nih.gov/pubmed/26505478}{26505478}]},
  abstract       = {Chronic inflammatory airway diseases, such as bronchial asthma and chronic obstructive pulmonary disease, are common respiratory disorders worldwide. Exacerbations of these diseases are frequent and worsen patients' respiratory condition and overall health. However, the mechanisms of exacerbation have not been fully elucidated. Recently, it was reported that interleukin (IL)-17A might play an important role in neutrophilic inflammation, which is characteristic of such exacerbations, through increased production of neutrophil chemoattractants. Therefore, we hypothesized that IL-17A was involved in the pathogenesis of acute exacerbation, due to viral infection in chronic inflammatory airway diseases. In this study, we assessed chemokine production by bronchial epithelial cells and investigated the underlying mechanisms. Comprehensive chemokine analysis showed that, compared with poly(I:C) alone, co-stimulation of BEAS-2B cells with IL-17A and poly(I:C) strongly induced production of such neutrophil chemoattractants as CXC chemokine ligand (CXCL)8, growth-related oncogene (GRO), and CXCL1. Co-stimulation synergistically induced CXCL8 and CXCL1 mRNA and protein production by BEAS-2B cells and normal human bronchial epithelial cells. Poly(I:C) induced chemokine expression by BEAS-2B cells mainly via Toll-like receptor 3/TIR-domain-containing adapter-inducing interferon-âÃ©Â¬â¤-mediated signals. The co-stimulation with IL-17A and poly(I:C) markedly activated the p38 and extracellular-signal-regulated kinase 1/2 pathway, compared with poly(I:C), although there was little change in nuclear factor-âÃ©Â¬â«B translocation into the nucleus or the transcriptional activities of nuclear factor-âÃ©Â¬â«B and activator protein 1. IL-17A promoted stabilization of CXCL8 mRNA in BEAS-2B cells treated with poly(I:C). In conclusion, IL-17A appears to be involved in the pathogenesis of chronic inflammatory airway disease exacerbation, due to viral infection by promoting release of neutrophil chemoattractants from bronchial epithelial cells.},
  endnotereftype = {Journal Article},
  owner          = {blaine-mooers},
  shorttitle     = {Interleukin-17A and Toll-Like Receptor 3 Ligand Poly(I:C) Synergistically Induced Neutrophil Chemoattractant Production by Bronchial Epithelial Cells},
}

