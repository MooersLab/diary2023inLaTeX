@Article{Ravindranath2015AutoDockFRAdvancesInProteinLigandDockingWithExplicitlySpecifiedBindingSiteFlexibility,
  author          = {Ravindranath, Pradeep Anand and Forli, Stefano and Goodsell, David S and Olson, Arthur J and Sanner, Michel F},
  journal         = {PLoS computational biology},
