  author          = {Biava, Mariangela and Porretta, Giulio Cesare and Poce, Giovanna and Supino, Sibilla and Forli, Stefano and Rovini, Michele and Cappelli, Andrea and Manetti, Fabrizio and Botta, Maurizio and Sautebin, Lidia and Rossi, Antonietta and Pergola, Carlo and Ghelardini, Carla and Vivoli, Elisa and Makovec, Francesco and Anzellotti, Paola and Patrignani, Paola and Anzini, Maurizio},
  journal         = {Journal of medicinal chemistry},
  title           = {Cyclooxygenase-2 inhibitors. 1,5-diarylpyrrol-3-acetic esters with enhanced inhibitory activity toward cyclooxygenase-2 and improved cyclooxygenase-2/cyclooxygenase-1 selectivity.},
